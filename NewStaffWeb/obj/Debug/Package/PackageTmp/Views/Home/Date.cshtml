
@{
    ViewBag.Title = "排班表";
}

<div class="row am-cf">
    <div class="am-u-sm-12 am-u-md-12 am-u-lg-12 widget-margin-bottom-lg">
        <div class="widget am-cf widget-body-lg">
            <div class="am-fr am-cf">
                <div class="widget-fluctuation-description-amount" am-cf>
                    <a href="http://www.pl-kj.com/all.htm" target="_blank" style="color:#5eb95e !important;">点我查看整月排班</a>
                </div>
            </div>
            <div class="widget-body  am-fl">
                <div class="am-scrollable-horizontal ">
                    <iframe src="http://www.pl-kj.com/1st.htm" style="width:100%;border:none;height:450px;" id="excel"></iframe>
                </div>
            </div>
        </div>
    </div>
</div>


<script type="text/javascript">

    var mydate = new Date();
    var d = mydate.getDate();
    var t = parseInt(d);

    if (t < 15 && t > 7) {
        frames.excel.src = "http://www.pl-kj.com/1st.htm";
    }
    if (t < 22 && t > 14) {
        frames.excel.src = "http://www.pl-kj.com/2nd.htm";
    }


</script>